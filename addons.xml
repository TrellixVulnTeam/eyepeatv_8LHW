<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.eyepeatv" version="6.0.6" name="[COLOR gold]EyePeaTV[/COLOR]" provider-name="EyePeaTV">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.addon.common" />
		<import addon="pvr.iptvsimple" />
		<import addon="plugin.video.iptv.recorder" />
		<import addon="plugin.video.SportsDevil" />
		<import addon="plugin.video.live.streamspro" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="startup">
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>EyePeaTV</summary>
        <description>EyePeaTV - Live TV / PPV / Sports / Kids / Movies / Entertainment at an affordable price for all.</description>
        <disclaimer></disclaimer>
        <language>en</language>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.video.F4mProxy" name="F4mProxy" version="2.8.8" provider-name="Shani">
    <requires>
        <import addon="xbmc.python" version="2.1.0" />
        <import addon="script.module.requests" />
    </requires>
    <extension point="xbmc.python.module" library="lib" />
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary lang="en">Proxy for f4m</summary>
        <description lang="en">Use this addon to play F4m v1 and v2 streams</description>
    </extension>
</addon>
</addons>
