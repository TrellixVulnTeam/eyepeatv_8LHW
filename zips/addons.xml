<addon id="plugin.video.iptv.recorder"
version="1.0.0"
name="IPTV Recorder"
provider-name="primaeval, Chychy,EyePeaTV">
<requires>
  <import addon="xbmc.python" version="2.20.0"/>
  <import addon="script.module.xbmcswift2" version="2.4.0"/>
